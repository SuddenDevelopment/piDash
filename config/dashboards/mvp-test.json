      "id": "flex-layout",
      "name": "Flex Layout",
      "layout": {
        "type": "flex",
        "direction": "column",
        "gap": 16,
        "padding": 16,
        "justify": "center",
        "align": "stretch"
      },
      "transitions": {
        "enter": {
          "type": "fade",
          "duration": 400
        }
      },
      "panels": [
        {
          "id": "flex-title",
          "type": "text",
          "text": "Flex Layout Example",
          "style": {
            "fontSize": "$3xl",
            "fontWeight": "bold",
            "color": "$primary",
            "textAlign": "center",
            "marginBottom": 16
          }
        },
        {
          "id": "flex-description",
          "type": "text",
          "text": "Items arranged using flexbox - can flow vertically or horizontally with gap and padding",
          "style": {
            "fontSize": "$md",
            "color": "$textSecondary",
            "textAlign": "center",
            "marginBottom": 24
          }
        }
      ]
    },
