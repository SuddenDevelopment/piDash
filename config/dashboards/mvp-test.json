  "navigation": {
    "initialPage": "flex-layout",
    "transitions": {
      "default": {
        "type": "slide",
        "direction": "left",
        "duration": 300
      }
    },
    "events": [
      {
        "type": "onSwipeLeft",
        "actions": [
          {
            "type": "navigateNext"
          }
        ]
      },
      {
        "type": "onSwipeRight",
        "actions": [
          {
            "type": "navigatePrevious"
          }
        ]
      },
      {
        "type": "onSchedule",
        "schedule": {
          "interval": 10000,
          "rotate": true
        },
        "actions": [
          {
            "type": "navigateNext"
          }
        ]
      }
    ]
  },
